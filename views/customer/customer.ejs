<nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="#">finance123</a>
    </div>
  </div>
</nav>
<div class="dropdown">
    <button class="btn btn primary dropdown-toggle" type="button" data-toggle="dropdown">Customers
    <span class="caret"></span></button>
    <ul class="dropdown-menu">
      <li><a href="/customer/add"><span class="glyphicon glyphicon-plus"></span></a></li>
      <% _.each(list, function(customer) { %>
        <li><span><%= customer.id %> - <%= customer.name %>
              <a href="/customer/destroy?id=<%=customer.id%>"><span class="glyphicon glyphicon-trash"></span></a>
              <a href="/customer/edit?id=<%=customer.id%>"><span class="glyphicon glyphicon-edit"></span></a>
              <a href="/customer/stock?id=<%=customer.id%>"><span class="glyphicon glyphicon-eur"></span></a>
            </span>
        </li>
      <% }) %>
    </ul>
</div>
<% if(JSON.stringify(list)=="{}") { %> <span id="suggestion"><--Add a customer!</span> <% } %>
<br>
<% if (part!==null) { cust = cust[0];%>
<%- partial('../partials/'+part,cust) %><% } %>
