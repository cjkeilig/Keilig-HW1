<div class="card card-outline-primary">
<form class="form-horizontal" role="form" action="updates">
<% for(var prop in cust) { %> 
    <% if(typeof(cust[prop])==="function" || prop==="createdAt" || prop==="id"||prop=="updatedAt" || prop==="stocks" || prop==="assets"){continue;} %>
    <div class="form-group">
      <label class="control-label col-sm-2" for="<%=prop%>">
      <%=prop%></label>
      <div class="col-sm-10">
        <input class="form-control" value="<%=cust[prop]%>" name="<%=prop%>"><br>
     </div>
    </div>
<% } %>
    <input hidden="true" value="<%=cust['id']%>" name="id">
    <button class="btn btn-default pull-right"><a href="/">Cancel</a></button>
    <input type="submit" class="btn btn-default pull-right">
</form>
</div>
    

